{
    "gitbook": "3.x.x",
    "title": "轻松学Java（Head First Java）",
    "description": "深入掌握Java, 不难",
    "language": "zh",
    "structure": {
        "readme": "README.md",
        "summary": "SUMMARY.md"
    },
    "plugins": ["donate"],
    "pluginsConfig": {
        "donate": {
            "wechat": "docs/images/611739062.jpg",
            "alipay": "docs/images/633086908.jpg",
            "button": "打赏"
        },
        "fontSettings": {
            "theme": "white",
            "family": "msyh",
            "size": 2
        },
        "plugins": [
            "yahei",
            "katex",
            "-search"
        ]
    },
    "pdf": {
        "pageNumbers": true, 
        "fontFamily": "Arial",
        "fontSize": 12,
        "paperSize": "a4",
        "headerTemplate": "_CHAPTER_ > _SECTION_",
        "margin": {
            "right": 22,
            "left": 26,
            "top": 28,
            "bottom": 22
        }
    }
}
